{
  "scripts": {
    "build": "yarn generate && rm -rf public/* && parcel build src/index.html -d public --no-cache",
    "generate": "graphql-codegen --config codegen-client.yml",
    "format": "prettier -w '{api,src}/**/*.{ts,tsx,graphql,yml,json}'"
  },
  "name": "space-api",
  "version": "1.0.0",
  "license": "MIT",
  "browserslist": [
    "last 1 Chrome version"
  ],
  "alias": {
    "react": "preact/compat",
    "react-dom/test-utils": "preact/test-utils",
    "react-dom": "preact/compat"
  },
  "devDependencies": {
    "@apollo/client": "^3.3.2",
    "@graphql-codegen/cli": "1.19.4",
    "@graphql-codegen/fragment-matcher": "2.0.1",
    "@graphql-codegen/typescript": "1.19.0",
    "@graphql-codegen/typescript-operations": "^1.17.12",
    "@graphql-codegen/typescript-react-apollo": "2.2.1",
    "@types/react": "^17.0.0",
    "@types/react-router-dom": "^5.1.6",
    "apollo3-cache-persist": "^0.9.1",
    "autoprefixer": "^9",
    "graphql": "^15.4.0",
    "graphql-tools": "^7.0.2",
    "parcel": "^1.12.4",
    "parcel-plugin-bundle-visualiser": "^1.2.0",
    "postcss": "^8.1.4",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-import": "^13.0.0",
    "preact": "^10.5.7",
    "preact-dom": "^1.0.1",
    "prettier": "^2.2.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-router-dom": "^5.2.0",
    "tailwindcss": "npm:@tailwindcss/postcss7-compat",
    "typescript": "^4.1.2"
  }
}
