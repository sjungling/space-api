{
  "name": "apollo-missions",
  "version": "1.0.0",
  "description": "",
  "main": "src/index.js",
  "scripts": {
    "start": "ts-node -P tsconfig.json src/index.ts",
    "generate": "yarn generate:schema generate:graphql-types",
    "generate:graphql-types": "graphql-codegen --config codegen.yml",
    "generate:watch": "graphql-codegen --watch --config codegen.yml",
    "generate:schema": "ts-node src/typedefs.ts",
    "prestart": "yarn data:load generate",
    "predev": "yarn data:load generate",
    "postinstall": "yarn data:load",
    "data:load": "knex seed:run",
    "format": "prettier -w **/*.{ts,tsx,graphql,yml,json}"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@apollo/client": "^3.2.7",
    "apollo-datasource": "^0.7.2",
    "apollo-server": "^2.19.0",
    "body-parser": "^1.19.0",
    "datasource-sql": "^1.3.0",
    "express": "^4.17.1",
    "graphql": "^15.4.0",
    "graphql-tools": "^7.0.1",
    "nodemon": "^2.0.6",
    "prettier": "^2.1.2",
    "react": "17.0.1",
    "react-dom": "^17.0.1",
    "sqlite3": "^5.0.0",
    "typescript": "^4.0.5"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "1.19.2",
    "@graphql-codegen/fragment-matcher": "2.0.0",
    "@graphql-codegen/introspection": "1.18.0",
    "@graphql-codegen/typescript": "1.17.11",
    "@graphql-codegen/typescript-document-nodes": "1.17.8",
    "@graphql-codegen/typescript-graphql-files-modules": "1.18.0",
    "@graphql-codegen/typescript-operations": "1.17.10",
    "@graphql-codegen/typescript-react-apollo": "2.1.1",
    "@graphql-codegen/typescript-resolvers": "^1.17.10",
    "@types/express": "^4.17.9",
    "@types/graphql": "^14.5.0",
    "@types/node": "^14.14.7",
    "@types/react": "^16.9.56",
    "@types/react-dom": "^16.9.9",
    "csv-parse": "^4.14.1",
    "knex": "^0.21.12",
    "parcel-bundler": "^1.12.4"
  }
}
