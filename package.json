{
  "scripts": {
    "vercel-build": "yarn load:data && yarn generate:schema && yarn generate:types:server",
    "build": "rm -rf public/* && yarn load:data && yarn generate:types:client && parcel build src/index.html -d public --no-cache",
    "schema:push": "npx apollo service:push --graph=SpaceAPI-o3aoab --key=service:SpaceAPI-o3aoab:BWqcjtneYvYy8nMk_QrWJA --variant=current     --localSchemaFile=./schema.graphql",
    "generate:types": "graphql-codegen --config codegen-server.yml && graphql-codegen --config codegen-client.yml",
    "generate:types:server": "graphql-codegen --config codegen-server.yml",
    "generate:schema": "ts-node scripts/generate-schema.ts",
    "generate:types:client": "yarn generate:schema && graphql-codegen --config codegen-client.yml",
    "format": "prettier -w '{api,src}/**/*.{ts,tsx,graphql,yml,json}'",
    "load:data": "knex seed:run"
  },
  "name": "apollo-vercel",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "alias": {
    "react": "preact/compat",
    "react-dom/test-utils": "preact/test-utils",
    "react-dom": "preact/compat"
  },
  "browserslist": [
    "last 1 Chrome version"
  ],
  "dependencies": {
    "@apollo/client": "^3.3.2",
    "@saeris/apollo-server-vercel": "^0.4.0",
    "apollo-datasource": "^0.7.2",
    "datasource-sql": "^1.3.0",
    "graphql": "^15.4.0",
    "graphql-tools": "^7.0.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "sqlite3": "^5.0.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "1.19.4",
    "@graphql-codegen/fragment-matcher": "2.0.1",
    "@graphql-codegen/typescript": "1.19.0",
    "@graphql-codegen/typescript-operations": "^1.17.12",
    "@graphql-codegen/typescript-react-apollo": "2.2.1",
    "@graphql-codegen/typescript-resolvers": "1.18.0",
    "@types/react": "^17.0.0",
    "autoprefixer": "^9",
    "csv-parse": "^4.14.1",
    "knex": "^0.21.12",
    "parcel": "^1.12.4",
    "parcel-plugin-bundle-visualiser": "^1.2.0",
    "postcss": "^8.1.4",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-import": "^13.0.0",
    "preact": "^10.5.7",
    "preact-dom": "^1.0.1",
    "prettier": "^2.2.1",
    "tailwindcss": "npm:@tailwindcss/postcss7-compat"
  }
}
