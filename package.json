    "generate": "graphql-codegen --config codegen-client.yml",
    "format": "prettier -w 'src/**/*.{ts,tsx,graphql,yml,json}' '{webpack,postcss,tailwind,vercel,.eslintrc}*.{js,json}' '*.{md,yml}'",
