{
    "compilerOptions": {
      "jsx": "react",
      "esModuleInterop": true,
      "allowSyntheticDefaultImports": true,
      "module": "esnext",
      "target": "esnext",
      // "lib": ["dom", "es2017"],
      "moduleResolution": "node",
      "resolveJsonModule": true,
      "noImplicitAny": false,
      "sourceMap": true,
      "skipLibCheck": true,
      "strictNullChecks": true,
      "outDir": "./public/",
      "typeRoots": ["./node_modules/@types", "src/@types"]
    },
    "include": ["src"],
    "exclude": ["**/node_modules", "**/.*", "scripts/**"]
  }
  